{
  "name": "bookmark_t",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "dev:extension": "turbo run dev --filter=extension",
    "build:extension": "turbo run build --filter=extension",
    "load:extension": "echo 'Open Chrome -> Extensions -> Developer mode -> Load unpacked -> select apps/extension/dist folder'"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "turbo": "^2.5.6",
    "typescript": "5.9.2"
  },
  "packageManager": "pnpm@10.17.1",
  "engines": {
    "node": ">=18"
  }
}
